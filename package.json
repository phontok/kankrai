  },
  "devDependencies": {
    "concurrently": "^3.5.1"
  },
  "dependencies": {
    "body-parser": "^1.18.2",
    "crypto": "^1.0.1",
    "express": "^4.16.2",
    "php-serialize": "^1.2.5"
  }
}
