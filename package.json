{
  "name": "kankrai-server",
  "version": "1.0.0",
  "description": "Backend server for the kankrai website",
  "main": "server/app.js",
  "author": "phontok",
  "license": "MIT",
  "private": true,
  "scripts": {
    "client": "cd client && yarn run start",
    "server": "node server/app.js",
