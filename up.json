{
  "$schema": "http://json.schemastore.org/up.json",
  "name": "kankrai",
  "profile": "phontok_apex",
  "regions": ["us-east-2"],
  "lambda": {
    "memory": 512
  },
  "hooks": {
    "build": "yarn run build"
  },
  "proxy": {
    "command": "npm run server"
  },
  "stages": {
    "development": {
      "domain": "dev.kankrai.com"
    },
    "production": {
      "domain": "kankrai.com"
    }
  }
}
